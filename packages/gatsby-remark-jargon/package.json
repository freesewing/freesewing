    "symlink": "mkdir -p ./node_modules/@freesewing && cd ./node_modules/@freesewing && ln -s -f ../../../* . && cd -",
    "start": "rollup -c -w"
  },
  "peerDependencies": {},
  "dependencies": {
    "remark-jargon": "^2.19.9"
  },
  "devDependencies": {},
  "files": [
    "dist/*",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6"
  },
  "rollup": {
    "exports": "default"
