  "devDependencies": {},
  "files": [
    "dist/*",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6"
